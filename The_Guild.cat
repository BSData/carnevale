    <rule id="6e17-cf3b-dda9-3fa0" name="Mob Rule" hidden="false">
      <description>When this character makes a Combat action, they gain +1 ATT for every other friendly character with Mob Rule who is in base contact with that enemy.</description>
    </rule>
    <rule id="fe7b-fb78-8f55-a449" name="Union" hidden="false">
      <description>When a character with this special rule attacks an enemy who is in base contact with at least 2 other friendly characters, it receives +1 ATT.</description>
    </rule>
